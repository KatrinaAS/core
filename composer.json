{
    "name": "lotgd/core",
    "description": "Core functionality for Legend of the Green Dragon, a text-based RPG game.",
    "license": "AGPL-3.0",
    "version": "0.4.1",
    "autoload": {
        "psr-4": {
            "LotGD\\Core\\": "src/",
            "LotGD\\Core\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/daenerys"
    ],
    "require": {
        "php": "^7.1.0",
        "composer/composer": "*",
        "gedmo/doctrine-extensions": "*",
        "doctrine/orm": "2.5.*",
        "monolog/monolog": "^1.12",
        "symfony/console": "^3.0",
        "symfony/yaml": "^3.0",
        "d11wtq/boris": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/austenmc/php-docblock-checker"
        }

    ],
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "phpunit/dbunit": "^2.0",
        "block8/php-docblock-checker": "2.0.0"
    }
}
